<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netcoreapp3.1</TargetFramework>
        <PackageId>VerdantSparks.LocationData.EntityTypes</PackageId>
        <Version>1.1.2</Version>
        <Authors>Ferry To</Authors>
        <Company>VerdantSparks</Company>
        <PackageDescription>Define types for storing location data that can be used in proximity queries.</PackageDescription>
        <RepositoryUrl>https://github.com/VerdantSparks/LocationData.EntityTypes</RepositoryUrl>
        <RootNamespace>LocationData</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.Azure.Cosmos" Version="3.9.1" />
      <PackageReference Include="MongoDB.Driver" Version="2.10.4" />
      <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    </ItemGroup>
</Project>
